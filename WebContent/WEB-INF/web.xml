<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>zhouqiangWeb</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>index.html</welcome-file>
	
	</welcome-file-list>
	<servlet>
		<servlet-name>SearchBook</servlet-name>
		<servlet-class>com.servlet.SearchBookServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>SearchBorrow</servlet-name>
		<servlet-class>com.servlet.SearchBorrowServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>SearchBorrowRecord</servlet-name>
		<servlet-class>com.servlet.SearchBorrowRecordServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>LogIn</servlet-name>
		<servlet-class>com.servlet.LogInServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>Back</servlet-name>
		<servlet-class>com.servlet.BackServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>BorrowBook</servlet-name>
		<servlet-class>com.servlet.BorrowServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ModifyUser</servlet-name>
		<servlet-class>com.servlet.ModifyUserServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>remark</servlet-name>
		<servlet-class>com.servlet.SubmitCommentServlet</servlet-class>
	</servlet>
		<servlet>
		<servlet-name>OrderBook</servlet-name>
		<servlet-class>com.servlet.OrderServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>BookServlet</servlet-name>
		<servlet-class>com.servlet.BookServlet</servlet-class>
	</servlet>



	<servlet-mapping>
		<servlet-name>SearchBook</servlet-name>
		<url-pattern>/SearchBook</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>SearchBorrow</servlet-name>
		<url-pattern>/SearchBorrow</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>SearchBorrowRecord</servlet-name>
		<url-pattern>/SearchBorrowRecord</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>LogIn</servlet-name>
		<url-pattern>/LogIn</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>Back</servlet-name>
		<url-pattern>/return_book</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>BorrowBook</servlet-name>
		<url-pattern>/BorrowBook</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ModifyUser</servlet-name>
		<url-pattern>/ModifyUser</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>remark</servlet-name>
		<url-pattern>/remark</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>OrderBook</servlet-name>
		<url-pattern>/OrderBook</url-pattern>
	</servlet-mapping>
		<servlet-mapping>
		<servlet-name>BookServlet</servlet-name>
		<url-pattern>/BookServlet</url-pattern>
	</servlet-mapping>
	
</web-app>